  <pre><code class="language-typescript" is:raw>import { SimplePool } from 'nostr-tools';

interface ListingSearchOptions {
  type?: ListingType;
  tags?: string[];
  location?: string;
  geohash?: string;
  author?: string;
  limit?: number;
