    filtered = events.filter((e) => {
      const locationTag = e.tags.find((t: string[]) => t[0] === 'location');
      return locationTag?.[1]?.includes(options.location!);
    });
  }

  return filtered.sort((a, b) => b.created_at - a.created_at);
}

